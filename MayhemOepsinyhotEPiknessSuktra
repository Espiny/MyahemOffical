-- Scripts:

local function CZXF_fake_script() -- KeyHolder.LocalScript 
	local script = Instance.new('LocalScript', KeyHolder)

	script.Parent.TextButton.MouseButton1Click:Connect(function()
		if script.Parent.KeyBox.Text == "W3%Kd#Rm7~O8&pW*qfBZ$sVno2$Y^F+P#qLaJ5yTXx!b@u&v$!"then
			pcall(function()
				script.Parent.KeyBox.Text = "Correct Key Loading..."
				wait(2)
				loadstring(game:HttpGet("https://raw.githubusercontent.com/Espiny/MayhemX/main/espinyshiiiiarsneal"))()
				wait(3)
				script.Parent.Parent.Parent.Parent.Parent:Destroy()
			end)
		else
			script.Parent.KeyBox.Text = "Incorrect Key"
			wait(2)
			script.Parent.KeyBox.Text = ""
		end
	end)
end
coroutine.wrap(CZXF_fake_script)()
local function LBZZM_fake_script() -- PlayerIcon.LocalScript 
	local script = Instance.new('LocalScript', PlayerIcon)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size48x48
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	local imageLabel = script.Parent
	imageLabel.Image = content
end
coroutine.wrap(LBZZM_fake_script)()
local function MNNMMG_fake_script() -- Title_4.LocalScript 
	local script = Instance.new('LocalScript', Title_4)

	script.Parent.Text = "Hello, " .. game.Players.LocalPlayer.Name
end
coroutine.wrap(MNNMMG_fake_script)()
local function WDIZK_fake_script() -- CloseKey.LocalScript 
	local script = Instance.new('LocalScript', CloseKey)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(WDIZK_fake_script)()
local function ARQU_fake_script() -- Main.Drag 
	local script = Instance.new('LocalScript', Main)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	local secondGui = script.Parent.DragFrame
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	secondGui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	secondGui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(ARQU_fake_script)()
